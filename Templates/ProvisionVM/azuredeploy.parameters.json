    "newVMName": {
      "type": "string",
      "defaultValue": "aswinwin20"
    },
    "labName": {
      "type": "string",
      "defaultValue": "Functional"
    },
    "size": {
      "type": "string",
      "defaultValue": "Standard_B2s"
    },
    "userName": {
      "type": "string",
      "defaultValue": "aswala"
    },
    "expirationDate": {
      "type": "string",
      "defaultValue": "2018-12-10T03:30:00.000Z"
    },
    "password": {
      "type": "securestring"
    },
    "Atom_installUsername": {
      "type": "string",
      "defaultValue": "aswala"
    },
    "Atom_installPassword": {
      "type": "securestring"
    },
    "Slack_installUsername": {
      "type": "string",
      "defaultValue": "aswala"
    },
    "Slack_installPassword": {
      "type": "securestring"
    },
